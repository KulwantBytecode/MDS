      <!-- cart_container_id start -->
      <div class="added-products-boxes" id="cart_container_id">
        {% for item in cart.items %}
          {% assign gift_pr = false %}
          {% if gift_product_add_id == item.variant.id %}
            {% assign gift_pr = true %}
          {% endif %}
          <div
            class="item-cart-fill-main0 efd cart_items_div kp-{{item.variant.id}}{% if gift_pr %} hidden_qty{% endif %}"
            data-id="{{item.variant.id}}"
            data-type="Backpacks"
            data-value="{{item.variant.title}}"
          >
            <!-- Loader Start -->
            <div class="cart__loading">
              <svg
                aria-hidden="true"
                focusable="false"
                role="presentation"
                class="icon icon-spinner"
                viewBox="0 0 20 20"
              >
                <path d="M7.229 1.173a9.25 9.25 0 1 0 11.655 11.412 1.25 1.25 0 1 0-2.4-.698 6.75 6.75 0 1 1-8.506-8.329 1.25 1.25 0 1 0-.75-2.385z" fill="#919EAB"></path>
              </svg>
            </div>
            <!-- Loader End -->
            <div class="cart-fill-product-image">
              <a class="cart-image" href="{{ item.url }}">
                <div class="cart-engrave engrave {{ item.product.type }}"></div>
                <img src="{{ item.variant | img_url: 'master' }}" alt="{{ item.title | escape }}">
              </a>
            </div>
            <div class="cart-fill-product-detail">
              <div class="cart-fill-detail-01">
                <a href="{{ item.url }}">
                  <p>{{ item.product.title }}</p>
                </a>
                {% unless item.product.has_only_default_variant %}
                  <span class="ajaxcart__product-meta">{{ item.variant.title }}</span>
                {% endunless %}
                {% assign price = item.final_price | times: item.quantity %}
                <p class="price">{{ price | money }}</p>
              </div>
              <div class="cart-filldiscription"></div>
              <div class="cart__props">
                <div class="cart__prop cart_text">
                  {% assign property_size = item.properties | size %}
                  {% if property_size > 0 %}
                    <span>Personalisation:</span>
                    {% for p in item.properties %}
                      {% assign first_character_in_key = p.first | truncate: 1, '' %}
                      {% unless p.last == blank or first_character_in_key == '_' %}
                        {% if p.last contains '/uploads/' %}
                          <a class="lightbox" href="{{ p.last }}">{{ p.last | split: '/' | last }}</a>
                        {% else %}
                          {% if forloop.first %}
                            <span
                              class="foil-option prsnlizer_color {{ p.last }}"
                              ddts="{{ p.last }}"
                              style="order: 3;"
                            ></span>
                          {% else %}
                            <span class="chess_text">{{ p.last }}</span>
                          {% endif %}
                        {% endif %}
                      {% endunless %}
                    {% endfor %}
                  {% endif %}
                </div>
              </div>
              <div class="wc_qty_box quantity-box">
                <div class="num-block skin-3">
                  <ul class="QuantityBox cart_p_quantity">
                    <li class="minus-box">
                      <a
                        href="javascript:void(0)"
                        data-line="2"
                        class="cart_item_minus minus ajaxcart__qty-adjust cart-slider-decrease-qty ajaxcart__qty--minus"
                      >
                        <img
                          src="//cdn.shopify.com/s/files/1/0619/2575/3081/t/5/assets/minu.svg?v=419654119375667803"
                          class="img-fluid"
                        >
                      </a>
                    </li>
                    <li class="input-box">
                      <input
                        readonly=""
                        type="number"
                        name="updates"
                        class="QuantityInput Quantity_cart_slider ajaxcart__qty-num"
                        value="{{ item.quantity }}"
                        min="0"
                      >
                    </li>
                    <li class="plus-box">
                      <a
                        href="javascript:void(0)"
                        variant_id="{{item.variant.id}}"
                        class="cart_item_plus plus ajaxcart__qty-adjust cart-slider-increase-qty ajaxcart__qty--plus"
                      >
                        <img
                          src="//cdn.shopify.com/s/files/1/0619/2575/3081/t/5/assets/plus.svg?v=14305593096000451169"
                          class="img-fluid"
                        >
                      </a>
                    </li>
                  </ul>
                </div>
                <div class="delate-cart-item">
                  <button
                    class="grey-5 header-cart-item__remove__btn cart_remove_item remove_cart_item"
                    data-line="2"
                  ></button>
                </div>
              </div>
            </div>
          </div>
        {% endfor %}
      </div>
