      <!-- MINI CART + MAKE IT A MATCH STYLING START -->
      <div class="cart__matching showaproducts upsel-pro-drawer {% if cart.item_count > 0 %} showaproducts {%  else %} continue_shop  {% endif %}">
        <a href="javascript:void(0);" class="matching-heading open text-center text-black">Make it a match</a>
        <div class="css-slider cart-matching-slider" style="display:flex;">
          <div class="cart__matching-loader">
            {%- include 'icon-spinner' -%}
          </div>
        </div>
      </div>
      <!-- MINI CART + MAKE IT A MATCH STYLING END -->

      <input type="hidden" id="gift_product_add_id" value="{{ gift_product_add_id }}">

      <div class="youmay-also-likeproduct youmay_also_likeproduct_fill">
        <h3>You may also like</h3>
        <div class="carousel cartlike-slider">
          {% assign collection = collections[settings.cart_collection] %}

          {%- for product in collection.products -%}
            {% if product.variants.first.inventory_quantity > 0 %}
              <div
                class="carousel-cell {{ product.type | handle | downcase }}"
                data-type="{{ product.type | handle | downcase }}"
              >
                <div class="product-grid3">
                  <div class="product-image3">
                    <a href="{{ product.url }}">
                      {% for image in product.images %}
                        <img
                          data-value="{{ image.alt | downcase }}"
                          class="{% if forloop.first %}pic-1 text-wvt-1{% endif %} new_imgs"
                          data-src="{{ image.src | img_url: 'master' }}"
                          alt="{{ product.title }}"
                        >
                      {% endfor %}
                    </a>
                    <div class="labelss">
                      {% for t in product.tags %}
                        {% if t contains 'badge:new' %}
                          <span class="product-new-label">New</span>
                        {% endif %}
                      {% endfor %}
                      {% assign cmp_price = product.compare_at_price %}
                      {% assign pdp_price = product.price %}
                      {% assign cal_price = pdp_price | times: 100 | divided_by: cmp_price | replace: '.0', '' %}
                      {% assign final_discount = 100 | minus: cal_price %}
                      {% if cmp_price > pdp_price %}
                        <span class="product-sale-label">{{ final_discount }}% OFF</span>
                      {% endif %}
                    </div>
                  </div>

                  <div class="product-content nav-new-p-content">
                    <div class="productnametitle">
                      <div class="title-namenam">
                        <h3 class="title">
                          <a href="{{ product.url }}">{{ product.title }}</a>
                        </h3>
                      </div>
                      <div class="pricetag">
                        <p>
                          <span>{{ product.price | money_with_currency }}</span>
                        </p>
                      </div>
                    </div>

                    <div class="varient-boxxx">
                      {%- if product.selected_or_first_available_variant -%}
                        <p>
                          <span> in {{ product.selected_or_first_available_variant.title }} </span>
                        </p>
                      {%- endif -%}
                    </div>

                    {%- form 'product',
                      product,
                      id: product_form_id,
                      class: 'form',
                      novalidate: 'novalidate',
                      data-type: 'add-to-cart-form'
                    -%}
                      <input
                        type="hidden"
                        name="id"
                        value="{{ product.selected_or_first_available_variant.id }}"
                        disabled
                      >

                      {% if product.variants.size > 1 %}
                        <div
                          id="custom_option_hide_cart"
                          class="custom_option_hide_cart"
                          data-index="option1"
                          name="id"
                          style="display:none !important"
                        >
                          {% for variant in product.variants %}
                            <span data-value="{{ variant.id }}" data-title="{{ variant.title | handle }}">
                              {{ variant.title }}
                            </span>
                          {% endfor %}
                        </div>
                      {% endif %}

                      <div class="product-form__buttons_pys">
                        <button
                          type="submit"
                          name="add"
                          class="cart_add_btn product-form__submit button button--full-width {% if block.settings.show_dynamic_checkout and product.selling_plan_groups == empty %}button--secondary{% else %}button--primary{% endif %}"
                          data-id="{{ product.selected_or_first_available_variant.id }}"
                          {% if product.selected_or_first_available_variant.available == false %}
                            disabled
                          {% endif %}
                        >
                          <span>
                            {%- if product.selected_or_first_available_variant.available -%}
                              {{ 'products.product.add_to_cart' | t }}
                            {%- else -%}
                              {{ 'products.product.sold_out' | t }}
                            {%- endif -%}
                          </span>
                          <div class="loading-overlay__spinner hidden">
                            <svg
                              aria-hidden="true"
                              focusable="false"
                              role="presentation"
                              class="spinner"
                              viewBox="0 0 66 66"
                              xmlns="http://www.w3.org/2000/svg"
                            >
                              <circle class="path" fill="none" stroke-width="6" cx="33" cy="33" r="30"></circle>
                            </svg>
                          </div>
                        </button>
                      </div>
                    {%- endform -%}
                  </div>
                </div>
              </div>
            {% endif %}
          {%- endfor -%}
        </div>
      </div>
    </div>
    <!-- content-cart-nav end -->
  </div>
  <!-- Mini Cart Body End -->

  <!-- Mini Cart Footer Start -->
  <div class="cart-footer-nav">
    <!-- gift card wrapper starts -->

    {% assign product = all_products[settings['gift-product']] %}
    {% for variant in product.variants %}
      {% assign current_variant = product.selected_or_first_available_variant %}
      {%- assign compare_at_price = variant.compare_at_price -%}
      {%- assign price = variant.price -%}
    {% endfor %}
    {% if settings.showgiftproduct %}
      {% assign is_gift_cart = false %}

      {% for item in cart.items %}
        {% if item.title == 'Make it a Gift - Gift Wrap' %}
          {% assign is_gift_cart = true %}
        {% endif %}
      {% endfor %}

      <!-- cart_gift_box start -->
       {% comment %}
      <div class="cart_gift_box">
        <div class="cart_gift_item">
          <div class="c_g_item_left">
            <figure>
              <img
                class="active"
                src="{{ product.featured_image | img_url: 'master' }}"
                alt="{{ product.title | escape }}"
                style="width:50%;"
              >
            </figure>
          </div>
          <div class="c_g_item_middle">
            <div class="c_g_pro_title">{{ product.title }}</div>
            <div class="text-box" data-maxlength="65">{{ product.description }}</div>
          </div>
          <div class="c_g_item_right">
            <div class="cart__price-wrapper price_matching_item">
              <span class="price price__upsell">
                <span class="upsell_deiscount"> {{ product.selected_or_first_available_variant.price | money }}</span>
                <span class="free_puzzle"> {{ product.selected_or_first_available_variant.compare_at_price }} </span>
              </span>
            </div>
            <form class="gift-cart-form">
              <a
                data-id="{{ product.variants.first.id }}"
                name="add"
                class="button cart_add_btn_ggift_card cart_add_btn_gift_cart"
              >
                <span data-add-to-cart-text="">+ Add</span>
                <span data-loader="" class="hide"></span>
                <span class="gift-spin hide" data-loader>
                  {%- include 'icon-spinner' -%}
                </span>
              </a>
            </form>
          </div>
        </div>
      </div>
     {% endcomment %}
    {% endif %}
    <!-- gift card wrapper End -->
    <!-- cart_gift_box end -->

    <!-- subotla-navm-boxes Start -->
    <div class="subotla-navm-boxes">
      {% comment %}
        <div class="cart-final-subtotla">
          <p class="p1 fzbig black">Shipping:</p>
          <p class="p1 fzbig black">FREE</p>
        </div>
      {% endcomment %}
      <div class="cart-final-subtotla cart_final_subtotal_price_wrap">
        <p class="p1 fzbig black">Total:</p>
        <p class="p1 fzbig black cart_final_subtotal_price">{{ cart.total_price | money }}</p>
      </div>

      <div class="cart-status cart-afterpay">
        <p>OR 4 easy payments of $<span class="cart-ap-price">0.00</span> with {% include 'afterpay-icon' %}</p>
      </div>

      <div class="button-finalnavm-box">
        <button type="submit" class="custom-btn custom_checkout_btn" name="checkout">Checkout</button>
        <button
          type="button"
          name="checkout"
          class="btn btn--payment cart__submit cart__submit-control hide checkout-loading"
          disabled="disabled"
        >
          <span>
            <svg class="icon icon-spinner">
              <path d="M7.229 1.173a9.25 9.25 0 1 0 11.655 11.412 1.25 1.25 0 1 0-2.4-.698 6.75 6.75 0 1 1-8.506-8.329 1.25 1.25 0 1 0-.75-2.385z"></path>
            </svg>
          </span>
        </button>
      </div>
      {% comment %}
              {% if additional_checkout_buttons %}
          {{ content_for_additional_checkout_buttons }}
        {% endif %}
      {% endcomment %}
    </div>
    <!-- subotla-navm-boxes End -->
  </div>
  <!-- Mini Cart Footer Start -->
</div>

<!-- Script For checkout and upsell cart slider -->
<script>
  $(document).ready(function(){
    if ($(window).width() < 480) {
    $('.custom_checkout_btn').on('click', function(){
    	window.location = '/checkout';
        window.setTimeout(function(){
          $('.custom_checkout_btn').removeClass('hide');
          $('.checkout-loading').addClass('hide');
        },2000)
    });
    }

    $('#cartpopupboxnav .upsel-pro-drawer h2').on('click', function(){
        	$(this).parents('.upsel-pro-drawer').toggleClass('opened');
        	$(this).next('.upsell-cart-slider').slideToggle();
    });
  });
</script>
