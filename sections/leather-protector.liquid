        {{ product.selected_or_first_available_variant.metafields.custom.new_custom_tag1.value }}
        </span>
        <span class="badge badge--bottom-left custtomtag2 product-tag {% if product.selected_or_first_available_variant.metafields.custom.new_custom_tag2.value != blank %}show{% else %}hide{% endif %}" var-titles="{{ variant.title }}">
        {{ product.selected_or_first_available_variant.metafields.custom.new_custom_tag2.value }}
        </span>
        <span class="badge badge--bottom-left custtomtag3 product-tag {% if product.selected_or_first_available_variant.metafields.custom.new_custom_tag3.value != blank %}show{% else %}hide{% endif %}" var-titles="{{ variant.title }}">
        {{ product.selected_or_first_available_variant.metafields.custom.new_custom_tag3.value }}
