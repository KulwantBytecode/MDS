<script>
  function formToJSON(elements) {
  	return [].reduce.call(elements, function (data, element) {
  		data[element.name] = element.value;
  		return data;
  	}, {});
  }
  function getUrlString(data) {
  	var urlParameters = Object.entries(data).map(function (e) {
  		return e.join('=');
  	}).join('&');

  	return urlParameters;
  }

  document.getElementById("newsContact").addEventListener("submit", function(e){
    e.preventDefault();
    var form_type = e.target.querySelector("[name=form_type]").value,
        inputs = e.target.querySelectorAll("[name]"),
        alert = e.target.querySelector('[data-alert="status"]'),
        alert_msgs = e.target.querySelector('.form-alerts');
    var action = e.target.getAttribute("action");
    if(e.target.querySelector("#emailData").value == ""){
      return false;
    }

    console.log("Form Action: " + action);
    console.log("Submitting " + form_type + " form...");

    fetch(action, {
          method: 'POST',
          body: getUrlString(formToJSON(inputs)),
          headers: {
              'Content-Type': 'application/x-www-form-urlencoded; charset=UTF-8',
              'Accept': 'text/html, */*; q=0.01',
              'X-Requested-With': 'XMLHttpRequest'
          }
      }).then(function(response) {
          console.log(response);
          console.log(response.status);
          document.getElementById("newsContact_aftermsg").style.display = "block";
