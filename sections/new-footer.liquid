            {% for link in linklists[section.settings.col2].links %}
              <li>
                <a href="{{link.url}}">{{ link.title }} </a>
              </li>
            {% endfor %}
          </ul>
        </div>
        <div class="footer-menu-innerbox account_block">
          <h5>{{ section.settings.text3 }}</h5>
          <ul>
            {% for link in linklists[section.settings.col3].links %}
              <li>
                <a href="{{link.url}}">{{ link.title }} </a>
              </li>
            {% endfor %}
          </ul>
